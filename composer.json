    "type": "library",
    "keywords": [ "common", "php", "dev", "tools", "lib", "modern", "ek9" ],
    "homepage": "https://github.com/leanphp/common-dev",
    "license": "MIT",
    "authors": [
        {
            "name": "ek9",
            "email": "dev@ek9.co",
            "homepage": "https://ek9.co"
        }
    ],
    "support": {
